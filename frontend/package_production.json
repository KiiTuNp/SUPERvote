  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "build:prod": "cross-env NODE_ENV=production GENERATE_SOURCEMAP=false craco build",
    "test": "craco test",
    "test:coverage": "craco test --coverage --watchAll=false",
    "test:ci": "cross-env CI=true craco test --coverage --watchAll=false --passWithNoTests",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "size": "size-limit",
    "lighthouse": "lighthouse-ci autorun",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "commit": "cz",
    "release": "semantic-release",
    "clean": "rimraf build dist coverage .nyc_output",
    "serve": "serve -s build -l 3000",
    "serve:prod": "cross-env NODE_ENV=production serve -s build -l 3000",
    "dev": "concurrently \"npm run start\" \"npm run test:e2e:open\"",
    "ci": "npm run lint && npm run typecheck && npm run test:ci && npm run build:prod"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "@typescript-eslint/recommended",
      "prettier"
    ],
    "plugins": [
      "react-hooks",
      "testing-library",
      "jest-dom"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn",
      "no-unused-vars": "warn",
      "@typescript-eslint/no-unused-vars": "warn",
      "prefer-const": "error",
      "no-var": "error"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/**/*.[jt]s?(x)",
          "**/?(*.)+(spec|test).[jt]s?(x)"
        ],
        "extends": [
          "plugin:testing-library/react",
          "plugin:jest-dom/recommended"
        ]
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie <= 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{json,css,md}": [
      "prettier --write"
    ]
  },
  "size-limit": [
    {
      "path": "build/static/js/*.js",
      "limit": "500 KB"
    },
    {
      "path": "build/static/css/*.css",
      "limit": "50 KB"
    }
  ],
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
